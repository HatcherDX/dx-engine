{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "compile": {
      "dependsOn": ["^build", "^compile"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "stub": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
